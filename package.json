{
  "name": "periodic-data-loader",
  "version": "0.1.0",
  "description": "Data loader with periodic call to load function.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "dataloader",
    "typescript"
  ],
  "scripts": {
    "test": "nyc mocha --reporter=spec -r tests/tshook.js tests/*.ts",
    "coveralls": "nyc report --reporter=text-lcov | ./node_modules/.bin/coveralls",
    "build": "tsc -p src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amoretspero/periodic-data-loader.git"
  },
  "author": "Jiung Hahm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amoretspero/periodic-data-loader/issues"
  },
  "homepage": "https://github.com/amoretspero/periodic-data-loader#readme",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.2",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.4"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "sourceMap": true,
    "instrument": true
  }
}